{
    "name": "template-ts-react",
    "version": "0.2.0",
    "private": true,
    "description": "Template project that includes React, TypeScript, React-Router, Sass and Webpack",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "author": {
        "name": "Dino Kacevenda",
        "email": "kacavendadino@gmail.com"
    },
    "keywords": [
        "react",
        "typescript",
        "webpack",
        "sass",
        "react-router",
        "redux",
        "tslint",
        "jest",
        "yarn"
    ],
    "repository": {
        "url": "https://github.com/Kaca992/template-ts-react"
    },
    "scripts": {
        "dev": "webpack-dev-server --debug --inline --progress --colors --open",
        "build": "cross-env NODE_ENV=production webpack -p --progress --colors",
        "clean": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command Remove-Item .\\lib, .\\dist -Force -Recurse",
        "remove-modules": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command Remove-Item .\\node_modules -Force -Recurse",
        "supportedb": "npx browserslist",
        "lint": "tslint --project . --fix",
        "test": "jest --verbose",
        "test:coverage": "jest --config .\\test\\config\\jest.config.coverage.js",
        "type-check": "npx tsc --noEmit"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "@testing-library/jest-dom": "^5.1.1",
        "@testing-library/react": "^9.4.1",
        "@types/classnames": "^2.2.9",
        "@types/jest": "^25.1.3",
        "@types/lodash": "^4.14.149",
        "@types/react": "^16.9.22",
        "@types/react-dom": "^16.9.5",
        "@types/react-redux": "^7.1.7",
        "@types/react-router": "^5.1.4",
        "@types/react-router-dom": "^5.1.3",
        "autoprefixer": "^9.7.4",
        "babel-loader": "^8.0.6",
        "cross-env": "^7.0.0",
        "css-loader": "^3.4.2",
        "file-loader": "^5.1.0",
        "filemanager-webpack-plugin": "^2.0.5",
        "fork-ts-checker-webpack-plugin": "^1.4.3",
        "hard-source-webpack-plugin": "^0.13.1",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^4.2.3",
        "jest": "^25.1.0",
        "jest-fetch-mock": "^3.0.1",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^4.13.1",
        "postcss-loader": "^3.0.0",
        "prettier": "^1.19.1",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.3",
        "ts-jest": "^25.2.1",
        "ts-loader": "^6.2.1",
        "tslint": "^6.0.0",
        "tslint-config-airbnb": "^5.11.2",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^4.2.0",
        "tslint-react-hooks": "^2.2.1",
        "tslint-sonarts": "^1.9.0",
        "typescript": "^3.8.2",
        "webpack": "^4.41.6",
