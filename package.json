{
    "name": "template-ts-react",
    "version": "0.1.0",
    "private": true,
    "description": "Template project that includes React, TypeScript, React-Router, Sass and Webpack v4",
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "author": {
        "name": "Dino Kacevenda",
        "email": "kacavendadino@gmail.com"
    },
    "keywords": [
        "react",
        "typescript",
        "webpack",
        "sass",
        "react-router",
        "redux",
        "tslint",
        "yarn"
    ],
    "repository": {
        "url": "https://github.com/Kaca992/template-ts-react"
    },
    "scripts": {
        "compile": "tsc",
        "dev": "webpack-dev-server --debug --output-pathinfo --history-api-fallback --hot --inline --progress --colors --port 3000 --open",
        "mobile-dev": "webpack-dev-server --debug --output-pathinfo --history-api-fallback --hot --inline --progress --colors --port 3000 --open --host {enter ip adress here}",
        "build": "set NODE_ENV=production&webpack --progress --colors",
        "clean-modules": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command Remove-Item .\\node_modules -Force -Recurse",
        "supportedb": "npx browserslist",
        "lint": "tslint --project . --fix",
        "test": "jest --verbose",
        "test:coverage": "jest --config .\\test\\config\\jest.config.coverage.js"
    },
    "license": "MIT",
